  
  
  /*
  * Test 30: 
  */
   describe('Rétt vinnumat raungreinakennara', function() {
    it("Guðrún Ingibjörg", function() {
      var kennari = new Kennari('Guðrún Ingibjörg')
      var a = new Afangi(['efn 103', 3, 17,'Raungreinar, efra þrep']);
      var b = new Afangi(['efn 203', 3, 19,'Raungreinar, efra þrep']);
      var c = new Afangi(['nær 103', 3, 26,'Almennar raungreinar']);
      var d = new Afangi(['nær 103', 3, 31,'Almennar raungreinar']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.ryra();
      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-170.84)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-176.18)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-173.76)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-189.12)).toBeLessThan(0.5);
    });
  });
  /*
  * Test 34: 
  */
   describe('Rétt vinnumat enskukennara (þrískert)', function() {
    it("Herdís", function() {
      var kennari = new Kennari('Herdís')
      var a = new Afangi(['a', 3, 21,'Erlend mál, efra þrep']);
      var b = new Afangi(['a', 3, 22,'Erlend mál, efra þrep']);
      var c = new Afangi(['c', 3, 17,'Erlend mál, neðra þrep']);
      var d = new Afangi(['a', 3, 21,'Erlend mál, efra þrep']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.ryra();
      var s = Math.abs(kennari.originalAfangar[0].vinnumat());
      s += Math.abs(kennari.originalAfangar[1].vinnumat());
      s += Math.abs(kennari.originalAfangar[3].vinnumat());
      expect(Math.abs(s-508)).toBeLessThan(0.5);
    });
  });



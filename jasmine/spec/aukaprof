  * Test 34: 
  */
   describe('Rétt vinnumat enskukennara (þrískert)', function() {
    it("Herdís", function() {
      var kennari = new Kennari('Herdís')
      var a = new Afangi(['a', 3, 21,'Erlend mál, efra þrep']);
      var b = new Afangi(['a', 3, 22,'Erlend mál, efra þrep']);
      var c = new Afangi(['c', 3, 17,'Erlend mál, neðra þrep']);
      var d = new Afangi(['a', 3, 21,'Erlend mál, efra þrep']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.ryra();
      var s = Math.abs(kennari.originalAfangar[0].vinnumat());
      s += Math.abs(kennari.originalAfangar[1].vinnumat());
      s += Math.abs(kennari.originalAfangar[3].vinnumat());
      expect(Math.abs(s-508)).toBeLessThan(0.5);
    });
  });


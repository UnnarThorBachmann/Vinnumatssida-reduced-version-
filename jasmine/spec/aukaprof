  
  
  /*
  * Test 30: 
  */
   describe('Rétt vinnumat raungreinakennara', function() {
    it("Guðrún Ingibjörg", function() {
      var kennari = new Kennari('Guðrún Ingibjörg')
      var a = new Afangi(['efn 103', 3, 17,'Raungreinar, efra þrep']);
      var b = new Afangi(['efn 203', 3, 19,'Raungreinar, efra þrep']);
      var c = new Afangi(['nær 103', 3, 26,'Almennar raungreinar']);
      var d = new Afangi(['nær 103', 3, 31,'Almennar raungreinar']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.ryra();
      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-170.84)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-176.18)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-173.76)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-189.12)).toBeLessThan(0.5);
    });
  });


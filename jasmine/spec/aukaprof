   describe('Rétt vinnumat stærðfræðikennara', function() {
    it("Jónína", function() {
      var kennari = new Kennari('Jónína')
      var a = new Afangi(['a', 3, 20,'Stærðfræði, efra þrep']);
      var b = new Afangi(['b', 3, 26,'Almenn braut']);
      var c = new Afangi(['c', 3, 24,'Stærðfræði, efra þrep']);
      var d = new Afangi(['d', 3, 21,'Stærðfræði, efra þrep']);
      var e = new Afangi(['e', 3, 30,'Stærðfræði, neðra þrep']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.addAfangi(e);
      kennari.ryra();

      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-169.34)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-180.02)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-172.01)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[4].vinnumat()-192)).toBeLessThan(0.5);
    });
  });

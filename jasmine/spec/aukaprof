   describe('Rétt vinnumat raungreinakennara', function() {
    it("Líney", function() {
      var kennari = new Kennari('Líney')
      var a = new Afangi(['a', 3, 19,'Raungreinar, neðra þrep']);
      var b = new Afangi(['b', 3, 20,'Raungreinar, neðra þrep']);
      var c = new Afangi(['c', 3, 19,'Almennar raungreinar']);
      var d = new Afangi(['c', 3, 21,'Almennar raungreinar']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.ryra();

      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-171)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-173.5)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-156.96)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-161.76)).toBeLessThan(0.5);
    });
  });

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.ryra();
      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-172.5)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-211.4)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-180.98)).toBeLessThan(0.5);
    });
  });

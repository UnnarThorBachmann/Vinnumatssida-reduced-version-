   describe('Frönskukennari rétt vinnumat', function() {
    it("Ingunn", function() {
      var kennari = new Kennari('Ingunn')
      var a = new Afangi(['a', 3, 20,'Erlend mál, neðra þrep']);
      var b = new Afangi(['b', 3, 17,'Erlend mál, neðra þrep']);
      var c = new Afangi(['c', 3, 14,'Erlend mál, neðra þrep']);
      var d = new Afangi(['d', 3, 10,'Erlend mál, efra þrep']);
      var e = new Afangi(['e', 3, 11,'Erlend mál, efra þrep']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.addAfangi(e);
      kennari.ryra();

      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-170)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-162.5)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-162.5)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-167.3)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[4].vinnumat()-167.3)).toBeLessThan(0.5);
    });

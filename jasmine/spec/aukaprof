   describe('Kennari í heilbrigðisskóli', function() {
    it("Inga", function() {
      var kennari = new Kennari('Inga')
      var a = new Afangi(['a', 3, 16,'Almennar raungreinar']);
      var b = new Afangi(['b', 3, 17,'Fagbóklegt']);
      var c = new Afangi(['c', 3, 12,'Verklegt']);
      var d = new Afangi(['a', 3, 26,'Almennar raungreinar']);
      var e = new Afangi(['c', 3, 12,'Verklegt']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.addAfangi(e);
      kennari.ryra();

      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-152.16)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-179.89)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-167.8)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-173.76)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[4].vinnumat()-167.712)).toBeLessThan(0.5);

    });
  });
  

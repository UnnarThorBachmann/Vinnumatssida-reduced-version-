      var a = new Afangi(['a', 3, 20,'Íslenska, efra þrep']);
      var b = new Afangi(['b', 3, 25,'Íslenska, efra þrep']);
      var c = new Afangi(['c', 3, 22,'Íslenska, neðra þrep']);
      var d = new Afangi(['a', 3, 15,'Íslenska, efra þrep']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.ryra();

      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-171.1776)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-191.66)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-179.65)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-163.48)).toBeLessThan(0.5);
    });
  });

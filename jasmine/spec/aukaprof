   describe('Spænskukennari rétt vinnumat', function() {
    it("Carmen", function() {
      var kennari = new Kennari('Carmen')
      var a = new Afangi(['a', 3, 17,'Erlend mál, neðra þrep']);
      var b = new Afangi(['b', 3, 25,'Erlend mál, neðra þrep']);
      var c = new Afangi(['c', 3, 6,'Erlend mál, efra þrep']);
      var d = new Afangi(['a', 3, 19,'Erlend mál, neðra þrep']);

      kennari.addAfangi(a);
      kennari.addAfangi(b);
      kennari.addAfangi(c);
      kennari.addAfangi(d);
      kennari.ryra();

      expect(Math.abs(kennari.originalAfangar[0].vinnumat()-156)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[1].vinnumat()-182.5)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[2].vinnumat()-167.3)).toBeLessThan(0.5);
      expect(Math.abs(kennari.originalAfangar[3].vinnumat()-160.8)).toBeLessThan(0.5);
    });
  });
